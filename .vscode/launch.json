{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "run runSchedule",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "runSchedule"
